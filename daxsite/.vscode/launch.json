{
    "version": "0.2.0",
    "configurations": [
            {
        "name": "Launch Local Chrome",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/src",
        "sourceMaps": true,
        "trace": true,
        "skipFiles": ["node_modules/**"]
      },
      {
        "name": "Attach to local Chrome",
        "type": "chrome",
        "request": "attach",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/src",
        "sourceMaps": true,
        "trace": true,
        "port": 9222,
        "skipFiles": ["node_modules/**"]
      },

      {
        "name": "Attach to Node.js",
        "type": "node",
        "request": "attach",
        "port": 9229,
        "restart": true,
        "skipFiles": ["<node_internals>/**"]
      }
    ]
  
  }
  